{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/**.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.svg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/officeonlineserver/breadcrumb/toc.json",
      "extendBreadcrumb": "true",
      "searchScope": ["Office Online Server"],
      "feedback_system": "GitHub",
      "feedback_github_repo": "MicrosoftDocs/OfficeDocs-OfficeOnlineServer",
      "feedback_product_url": "https://github.com/MicrosoftDocs/OfficeDocs-OfficeOnlineServer/issues"     
    },
    "fileMetadata": {},
    "template": [],
    "dest": "OfficeDocs-OfficeOnlineServer"
  }
}
